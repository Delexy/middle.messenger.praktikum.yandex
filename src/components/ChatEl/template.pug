.chat-el(class=(isActive ? 'is-active' : ''))
  | !{UserPhoto}
  .chat-el__content 
    span.chat-el__name= title
      if lastMessage
        span.chat-el__text(class=(isUserMessageLast ? 'chat-el__text_last-user' : ''))= lastMessage.content || '' 
    if lastMessage
      span.chat-el__time= lastMessage.time
  if messageCount > 0
    span.chat-el__msg-count= messageCount
